{
  "name": "site",
  "version": "0.0.1",
  "author": "Withcenter, Inc.",
  "homepage": "http://www.ontue.com/",
  "scripts": {
    "ng": "ng",
    "start": "ionic serve",
    "build": "ionic build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "s": "ng serve --disable-host-check",
    "serve:local": "ng serve --disable-host-check --configuration=local-backend-server",
    "serve:hmr": "ng serve --disable-host-check --configuration=hmr",
    "bt": "ng build --prod --named-chunks=true",
    "publish": "npm run build:prod && npm run scp",
    "publish:gitpage": "ng build --prod --output-path docs --base-href=/site/ && git add --all && git commit -m 'buildtest' && git push",
    "publish:all": "npm run build:prod && npm run scp:all",
    "build:prod": "ng build --prod",
    "scp": "cd www && scp *.js *.css *.html ontue@ontue.com:./www && cd assets && scp -r * ontue@ontue.com:./www/assets",
    "scp:all": "cd www && scp -r * ontue@ontue.com:./www",
    "delete-remote-js": "ssh ontue@ontue.com 'rm -f www/*.js'",
    "ssl": "ng serve --disable-host-check --configuration=local-backend-server --ssl --ssl-key './tmp/ssl/wild-katalkenglish/private.key' --ssl-cert './tmp/ssl/wild-katalkenglish/certificate.crt' --port 9443"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.4",
    "@angular/cdk": "^6.0.4",
    "@angular/common": "^6.0.4",
    "@angular/core": "^6.0.4",
    "@angular/forms": "^6.0.4",
    "@angular/http": "^6.0.4",
    "@angular/material": "^6.0.4",
    "@angular/platform-browser": "^6.0.4",
    "@angular/platform-browser-dynamic": "^6.0.4",
    "@angular/router": "^6.0.4",
    "@ionic/angular": "4.0.0-alpha.3",
    "@ionic/schematics-angular": "^1.0.0-rc.5",
    "bootstrap": "^4.1.1",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.3",
    "firebase": "^5.0.4",
    "hammerjs": "^2.0.8",
    "rxjs": "6.1.0",
    "rxjs-compat": "^6.1.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.25"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.6.0",
    "@angular-devkit/core": "0.6.0",
    "@angular-devkit/schematics": "0.6.0",
    "@angular/cli": "^6.0.4",
    "@angular/compiler": "^6.0.4",
    "@angular/compiler-cli": "^6.0.4",
    "@angular/language-service": "^6.0.4",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "^2.8.7",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.10.11",
    "codelyzer": "~4.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~6.0.2",
    "tslint": "~5.10.0",
    "typescript": "~2.7.2"
  },
  "description": "An Ionic project"
}
